{
  "manifest_version": 2,

  "name": "w3z | shorten url | chrome extension",
  "description": "w3z extension allows you to shorten urls",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
